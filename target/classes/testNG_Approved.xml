<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Orion TimeSheet Automation Test Suite">

	<!--  Note : If Build tool is Maven in Jenkins, default directory of the extent report changes from 
		  /test-output folder to <workspace>/target/surefire-reports -->


	<listeners>
		<listener class-name="com.orion.qa.ExtentReportListener.ExtentReportListener" />
	</listeners>
	
	<test
		name="OrionTimeSheet_Chrome_Test_ApprovedTimeSheet_CancelFunctionality">
		<parameter name="Browser" value="chrome" />
		<parameter name="ClassName" value="Test_ApprovedTimeSheet_CancelFunctionality" />
		<classes>
			<class
				name="com.orion.qa.testcases.Test_ApprovedTimeSheet_CancelFunctionality" />
		</classes>
	</test> 
	
	<test
		name="OrionTimeSheet_Chrome_Test_ApprovedTimeSheet_SaveFunctionality">
		<parameter name="Browser" value="chrome" />
		<parameter name="ClassName" value="Test_ApprovedTimeSheet_SaveFunctionality" />
		<classes>
			<class
				name="com.orion.qa.testcases.Test_ApprovedTimeSheet_SaveFunctionality" />
		</classes>
	</test>

	<test
		name="OrionTimeSheet_Chrome_Test_ApprovedTimeSheet_SubmitFunctionality">
		<parameter name="Browser" value="chrome" />
		<parameter name="ClassName" value="Test_ApprovedTimeSheet_SubmitFunctionality" />
		<classes>
			<class
				name="com.orion.qa.testcases.Test_ApprovedTimeSheet_SubmitFunctionality" />
		</classes>
	</test>
	
</suite>