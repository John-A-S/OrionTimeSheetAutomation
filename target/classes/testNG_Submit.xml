<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Orion TimeSheet Automation Test Suite">
	<listeners>
		<listener class-name="com.orion.qa.ExtentReportListener.ExtentReportListener" />
	</listeners>

	<!-- SUBMITTED TIMESHEET CANCEL -->
	
	<test
		name="OrionTimeSheet_Chrome_Test_SubmittedTimeSheet_CancelFunctionality">
		<parameter name="Browser" value="chrome" />
		<parameter name="ClassName" value="Test_SubmittedTimeSheet_CancelFunctionality" />
		<classes>
			<class
				name="com.orion.qa.testcases.Test_SubmittedTimeSheet_CancelFunctionality" />
		</classes>
	</test>
		
	<!-- SUBMITTED TIMESHEET SAVE -->
	 
	<test name="OrionTimeSheet_Chrome_Test_SubmittedTimeSheet_SaveFunctionality">
		<parameter name="Browser" value="chrome" />
		<parameter name="ClassName" value="Test_SubmittedTimeSheet_SaveFunctionality" />
		<classes>
			<class
				name="com.orion.qa.testcases.Test_SubmittedTimeSheet_SaveFunctionality" />
		</classes>
	</test>
	
	<!-- SUBMITTED TIMESHEET SUBMIT -->
	 
	<test
		name="OrionTimeSheet_Chrome_Test_SubmittedTimeSheet_SubmittedFunctionality">
		<parameter name="Browser" value="chrome" />
		<parameter name="ClassName" value="Test_SubmittedTimeSheet_SubmitFunctionality" />
		<classes>
			<class
				name="com.orion.qa.testcases.Test_SubmittedTimeSheet_SubmitFunctionality" />
		</classes>
		
	</test>
	 	
</suite>