<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite
	name="Orion TimeSheet Automation - Verify Cancel, Save, Submit functionalities in Rejected timesheet">

	<!-- Note : If Build tool is Maven in Jenkins, default directory of the 
		extent report changes from /test-output folder to <workspace>/target/surefire-reports -->

	<listeners>
		<listener class-name="com.orion.qa.ExtentReportListener.ExtentReportListener" />
	</listeners>
	
	<test
		name="OrionTimeSheet_Chrome_Test_RejectedTimeSheet_CancelFunctionality">
		<parameter name="Browser" value="chrome" />
		<parameter name="ClassName" value="Test_RejectedTimeSheet_CancelFunctionality" />
		<classes>
			<class
				name="com.orion.qa.testcases.Test_RejectedTimeSheet_CancelFunctionality" />
		</classes>
	</test>
 	
	 
	<test name="OrionTimeSheet_Chrome_Test_RejectedTimeSheet_SaveFunctionality">
		<parameter name="Browser" value="chrome" />
		<parameter name="ClassName" value="Test_RejectedTimeSheet_SaveFunctionality" />
		<classes>
			<class
				name="com.orion.qa.testcases.Test_RejectedTimeSheet_SaveFunctionality" />
		</classes>
	</test> 
	
	<test
		name="OrionTimeSheet_Chrome_Test_RejectedTimeSheet_SubmitFunctionality">
		<parameter name="Browser" value="chrome" />
		<parameter name="ClassName" value="Test_RejectedTimeSheet_SubmitFunctionality" />
		<classes>
			<class
				name="com.orion.qa.testcases.Test_RejectedTimeSheet_SubmitFunctionality" />
		</classes>
	</test>    

</suite>